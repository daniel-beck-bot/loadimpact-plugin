<?xml version="1.0" encoding="utf-8"?>
<j:jelly xmlns:j="jelly:core" xmlns:f="/lib/form">
    <f:invisibleEntry>
        <f:textbox field="id"/>
    </f:invisibleEntry>
    <f:entry title="${%Description}" field="description">
        <f:textbox/>
    </f:entry>
    <f:entry title="${%Token}" field="apiToken">
        <f:textbox/>
    </f:entry>
    <f:validateButton title="${%Validate}" progress="${%Progress}"
            method="validateApiToken" with="apiToken"
            />
</j:jelly>
